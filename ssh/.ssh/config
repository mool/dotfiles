Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent yes
  PubkeyAcceptedKeyTypes=+ssh-rsa
  HostKeyAlgorithms=+ssh-rsa

Host mkt-wan-core
  HostName 168.205.121.1
  Port 22000
  User admin

Host 192.168.*
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host 10.*
  HostKeyAlgorithms=+ssh-dss,ssh-rsa
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host 172.16.*
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
